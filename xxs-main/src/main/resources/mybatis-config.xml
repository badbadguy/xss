<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"  
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<settings> 
        <setting name="cacheEnabled" value="true" /><!-- 全局映射器启用缓存 -->   
        <setting name="useGeneratedKeys" value="true" /> 
        <setting name="defaultExecutorType" value="REUSE" />
		<setting name="callSettersOnNulls" value="true"/>
    </settings>

	<typeAliases>
		<typeAlias type="com.fh.entity.system.User" alias="User"/>
		<typeAlias type="com.fh.entity.system.Role" alias="Role"/>
		<typeAlias type="com.fh.entity.system.Menu" alias="Menu"/>
		<typeAlias type="com.fh.entity.system.Dictionaries" alias="Dictionaries"/>
		<typeAlias type="com.fh.entity.system.Department" alias="Department"/>
		
		<!-- 这里添加生成的实体类 -->
		<typeAlias type="com.fh.util.PageData" alias="pd"/>
		<!-- 分页 -->
		<typeAlias type="com.fh.util.Page" alias="Page"/>
		<!--查询条件-->
		<typeAlias type="com.fh.entity.wx.WxUserInfoVO" alias="wxuserinfoVO"/>

		<!--保存条件-->
		<typeAlias type="com.fh.entity.wx.WxUserInfoPO" alias="wxuserinfoPO"/>
		<typeAlias type="com.fh.entity.wx.WxTemplateMsgPO" alias="wxTemplateMsgPO"/>
	</typeAliases>
	
	<plugins>
		<plugin interceptor="com.fh.util.PagePlugin">
			<property name="dialect" value="mysql"/>
			<property name="pageSqlId" value=".*listPage.*"/>
		</plugin>
	</plugins>
	
</configuration>